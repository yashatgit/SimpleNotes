{
  "manifest_version": 2,
  "name": "Simple Notes",
  "description": "A simple app to save Notes in browser history",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Add a Note!",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}